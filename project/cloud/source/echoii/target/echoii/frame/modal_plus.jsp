<!-- main-btn-plus-Modal -->

<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal"
		aria-hidden="true">×</button>
	<h3 id="main-btn-plus-Label">新建文件夹任务</h3>
</div>
<div class="modal-body">
	<div class="row span12">
		<div class="span3">
			<p>新建文件夹名称</p>
		</div>
		<div class="span8 ">
			<input id="plus-fold-name" class="input-xlarge" type="text"
				maxlength="15" placeholder="新建文件名称" />
		</div>
	</div>
</div>
<div class="modal-footer">
	<button id="plus-modal-save" class="btn btn-primary">保存</button>
</div>




